{
    "configurations": [
        {
            "name": "Node: Current File",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Browser: Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/metaverse",
            "userDataDir": "C:/Workspace/.vscode/chrome-debug-profile",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
        },
        {
            "name": "Browser: Edge",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/metaverse",
            "userDataDir": "C:/Workspace/.vscode/edge-debug-profile",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
        },
        {
            "name": "Vite: dev-server",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "run",
                "dev"
            ]
        },
        {
            "name": "Python: prod-preview",
            "type": "debugpy",
            "request": "launch",
            "module": "http.server",
            "args": [
                "4173",
                "-d",
                "./dist/"
            ],
            "console": "integratedTerminal"
        }
    ]
}